<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Grats"
    xmlns:model="using:Grats.Model">
    <model:TemplatesCollection x:Key="DefaultTemplates">
        <model:Template IsEmbedded="True">
            <model:Template.Text>
                Дорог^пол{м:ой, ж:ая, ой} ^имя ^фамилия, поздравляю тебя с днем рождения
            </model:Template.Text>
            <model:Template.Name>
                День рождения
            </model:Template.Name>
        </model:Template>
    </model:TemplatesCollection>
</ResourceDictionary>
